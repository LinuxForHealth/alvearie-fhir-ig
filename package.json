{
  "name": "alvearie.fhir.ig",
  "version": "0.1.1",
  "canonical": "http://alvearie.github.io/alvearie-fhir-ig/0.1.0",
  "url": "http://alvearie.github.io/alvearie-fhir-ig/0.1.0",
  "title": "Alvearie FHIR Implementation Guide",
  "description": "Alvearie FHIR Profiles",
  "devDependencies": {
    "fsh-sushi": "^0.16.0"
  },
  "scripts": {
    "sushi": "sushi data",
    "sushi_snapshot": "sushi -s data",
    "ig_init": "cp -r data/ig-data/alvearie-template build/ && cd build && yes | ./_updatePublisher.sh && ./_genonce.sh",
    "ig_init_win": "xcopy \"data\\ig-data\\alvearie-template\" \"build\\alvearie-template\" /s /i /y /q && cd build && _updatePublisher.bat && _genonce.bat",
    "ig": "cp -r data/ig-data/alvearie-template build/ && cd build && ./_genonce.sh",
    "ig_win": "xcopy \"data\\ig-data\\alvearie-template\" \"build\\alvearie-template\" /s /i /y /q && cd build && _genonce.bat",
    "build_snapshot": "mkdir -p ./src/main/resources/com/ibm/fhir/cdm && tar -C ./src/main/resources/com/ibm/fhir/cdm/ -xf ./build/output/package.tgz && mvn clean install",
    "copy_snapshot": "mkdir -p ./target/site && cp target/*.jar build/output && cp target/*.jar target/site",
    "copy_snapshot_win": "xcopy target\\*.jar build\\output &&x copy target\\*.jar target\\site",
    "zip_snapshot": "cd target && zip -ur ../build/output/full-ig.zip site/*.jar",
    "zip_snapshot_win": "cd target && 7z a ..\\build\\output\\full-ig.zip site/*.jar",
    "copy_version": "mkdir -p build/output/${npm_package_version} && rsync -ahcv build/output/ build/output/${npm_package_version} --delete",
    "copy_version_win": "xcopy build\\output build\\output\\${npm_package_version} /s /i /y /q",
    "copy_docs": "mkdir -p docs && rsync -ahcv build/output/ docs --delete",
    "copy_docs_win": "xcopy \"build\\output\" \"docs\" /s /i /y /q",
    "all": "npm run sushi && npm run ig_init && npm run build_snapshot && npm run copy_snapshot && npm run zip_snapshot && npm run copy_version",
    "all_win": "npm run sushi && npm run ig_init && npm run build_snapshot && npm run copy_snapshot_win && npm run zip_snapshot && npm run copy_version_win"
  },
  "language": "en",
  "author": "Alvearie Team",
  "maintainers": [
    {
      "name": "Tom Eggebraaten",
      "email": "tegge@us.ibm.com"
    },
    {
      "name": "Kim Kenna",
      "email": "sloand@us.ibm.com"
    }
  ],
  "license": "CC0-1.0"
}
